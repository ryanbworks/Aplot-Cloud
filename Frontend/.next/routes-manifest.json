{
  "version": 3,
  "pages404": true,
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        }
      ],
      "regex": "^(?:/(.*))(?:/)?$"
    }
  ],
  "rewrites": {
    "beforeFiles": [],
    "afterFiles": [],
    "fallback": []
  },
  "dynamicRoutes": [
    {
      "page": "/dashboard/suporte/ticket/[id]",
      "regex": "^/dashboard/suporte/ticket/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/dashboard/suporte/ticket/(?<nxtPid>[^/]+?)(?:/)?$"
    },
    {
      "page": "/suporte/base-conhecimento/[categoria]",
      "regex": "^/suporte/base\\-conhecimento/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPcategoria": "nxtPcategoria"
      },
      "namedRegex": "^/suporte/base\\-conhecimento/(?<nxtPcategoria>[^/]+?)(?:/)?$"
    },
    {
      "page": "/suporte/base-conhecimento/[categoria]/[slug]",
      "regex": "^/suporte/base\\-conhecimento/([^/]+?)/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPcategoria": "nxtPcategoria",
        "nxtPslug": "nxtPslug"
      },
      "namedRegex": "^/suporte/base\\-conhecimento/(?<nxtPcategoria>[^/]+?)/(?<nxtPslug>[^/]+?)(?:/)?$"
    }
  ],
  "staticRoutes": [
    {
      "page": "/",
      "regex": "^/(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/(?:/)?$"
    },
    {
      "page": "/_not-found",
      "regex": "^/_not\\-found(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/_not\\-found(?:/)?$"
    },
    {
      "page": "/auth/login",
      "regex": "^/auth/login(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/auth/login(?:/)?$"
    },
    {
      "page": "/auth/register",
      "regex": "^/auth/register(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/auth/register(?:/)?$"
    },
    {
      "page": "/dashboard",
      "regex": "^/dashboard(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard(?:/)?$"
    },
    {
      "page": "/dashboard/conta",
      "regex": "^/dashboard/conta(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/conta(?:/)?$"
    },
    {
      "page": "/dashboard/seguranca",
      "regex": "^/dashboard/seguranca(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/seguranca(?:/)?$"
    },
    {
      "page": "/dashboard/suporte/novo-ticket",
      "regex": "^/dashboard/suporte/novo\\-ticket(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/suporte/novo\\-ticket(?:/)?$"
    },
    {
      "page": "/dashboard/suporte/ticket",
      "regex": "^/dashboard/suporte/ticket(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/suporte/ticket(?:/)?$"
    },
    {
      "page": "/favicon.ico",
      "regex": "^/favicon\\.ico(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/favicon\\.ico(?:/)?$"
    },
    {
      "page": "/hospedagem/minecraft",
      "regex": "^/hospedagem/minecraft(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/hospedagem/minecraft(?:/)?$"
    },
    {
      "page": "/robots.txt",
      "regex": "^/robots\\.txt(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/robots\\.txt(?:/)?$"
    },
    {
      "page": "/sitemap.xml",
      "regex": "^/sitemap\\.xml(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/sitemap\\.xml(?:/)?$"
    },
    {
      "page": "/status",
      "regex": "^/status(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/status(?:/)?$"
    },
    {
      "page": "/suporte/base-conhecimento",
      "regex": "^/suporte/base\\-conhecimento(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/suporte/base\\-conhecimento(?:/)?$"
    }
  ],
  "dataRoutes": [],
  "rsc": {
    "header": "rsc",
    "varyHeader": "rsc, next-router-state-tree, next-router-prefetch, next-router-segment-prefetch",
    "prefetchHeader": "next-router-prefetch",
    "didPostponeHeader": "x-nextjs-postponed",
    "contentTypeHeader": "text/x-component",
    "suffix": ".rsc",
    "prefetchSuffix": ".prefetch.rsc",
    "prefetchSegmentHeader": "next-router-segment-prefetch",
    "prefetchSegmentSuffix": ".segment.rsc",
    "prefetchSegmentDirSuffix": ".segments"
  },
  "rewriteHeaders": {
    "pathHeader": "x-nextjs-rewritten-path",
    "queryHeader": "x-nextjs-rewritten-query"
  }
}